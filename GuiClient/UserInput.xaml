<Window x:Class="GuiClient.UserInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuiClient"
        mc:Ignorable="d"
        Title="UserInput" Height="131" Width="206">
    <Grid>
        <Label x:Name="Label" Content="Input property" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox" Margin="10,0,10,35" TextWrapping="Wrap" Height="22" VerticalAlignment="Bottom"/>
        <Button x:Name="ButtonOk" Content="Ok" Margin="0,0,10,10" HorizontalAlignment="Right" Width="75" Height="21" VerticalAlignment="Bottom" Click="Button_Click"/>

    </Grid>
</Window>
